{
    "write_flash_args" : [ "--flash_mode", "dio",
                           "--flash_size", "detect",
                           "--flash_freq", "80m" ],
    "flash_settings" : {
        "flash_mode": "dio",
        "flash_size": "detect",
        "flash_freq": "80m"
    },
    "flash_files" : {
        "0xe000" : "partition_table/partition-table.bin",
        "0x38b000" : "ota_data_initial.bin",
        "0x0" : "bootloader/bootloader.bin",
        "0x10000" : "$<TARGET_PROPERTY:__idf_build_target,EXECUTABLE_NAME>.bin"
    },
    "partition_table" : { "offset" : "0xe000", "file" : "partition_table/partition-table.bin", "encrypted" : "true" },
    "otadata" : { "offset" : "0x38b000", "file" : "ota_data_initial.bin", "encrypted" : "true" },
    "bootloader" : { "offset" : "0x0", "file" : "bootloader/bootloader.bin", "encrypted" : "true" },
    "app" : { "offset" : "0x10000", "file" : "$<TARGET_PROPERTY:__idf_build_target,EXECUTABLE_NAME>.bin", "encrypted" : "true" },
    "extra_esptool_args" : {
        "after"  : "no_reset",
        "before" : "default_reset",
        "stub"   : true,
        "chip"   : "esp32c3"
    }
}
